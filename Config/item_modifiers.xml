<configs>

    <!-- Remove all blocked_tags restrictions -->
    <set xpath="/item_modifiers/item_modifier/@blocked_tags">
        noMods</set>

    <!-- Set all armor/clothing installable_tags to be applicable to both -->
    <set
        xpath="/item_modifiers/item_modifier[contains(@installable_tags, 'armor') or contains(@installable_tags, 'pocketMod')]/@installable_tags">
        armor,pocketMod</set>

    <!-- Add speed buffs to modArmorImprovedFittings and modArmorCustomizedFittings -->

    <append xpath="/item_modifiers/item_modifier[@name='modArmorImprovedFittings']/effect_group">
        <passive_effect name="RunSpeed" operation="perc_add" value=".03" />
    </append>

    <append xpath="/item_modifiers/item_modifier[@name='modArmorCustomizedFittings']/effect_group">
        <passive_effect name="RunSpeed" operation="perc_add" value=".05" />
    </append>

